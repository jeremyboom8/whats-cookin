<%= simple_form_for @listing do |f| %>
  <%= f.input :title, placeholder: "Monkey pants"%>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :price %>
  <%= f.input :photo, as: :attachinary, id: "upload-photo" %>
  <%= f.submit "Submit", class: "btn btn-primary btn-block" %>
<% end %>

